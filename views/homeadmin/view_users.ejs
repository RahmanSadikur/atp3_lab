<!DOCTYPE html>
<html>
<head>
	<title>User List</title>
</head>
<body>
	<div>
			<h2>User List</h2> 		
	</div>

	<a href="/admin">Back</a>|
	<a href="/logout">Logout</a><br>

	<table border="1">
		<tr>
			<td>ID</td>
			<td>Employee NAME</td>
			<td>Company NAME</td>
			<td>Contract No</td>
			<td>USERNAME</td>
			<td>PASSWORD</td>
			<td>TYPE</td>
			<td>ACTION</td>
		</tr>
	<% for(var i=0; i < userlist.length; i++){ %>
		<tr>
			<td><%= userlist[i].Eid %></td>
			<td><%= userlist[i].Ename %></td>
			<td><%= userlist[i].Cname %></td>
			<td><%= userlist[i].ContractNo %></td>
			<td><%= userlist[i].username %></td>
			<td><%= userlist[i].password %></td>
			<td><%= userlist[i].Utid %></td>
			<td>
				<a href="/admin/edit/<%= userlist[i].Eid %>">EDIT</a> | 
				<a href="/admin/delete/<%= userlist[i].Eid %>">DELETE</a> 
			</td>
		</tr>
	<%  } %>
	</table>

</body>
</html>